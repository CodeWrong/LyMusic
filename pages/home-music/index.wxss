/* pages/home-music/index.wxss */
page{
    padding: 0 20rpx;
    box-sizing: border-box;
}



/* 轮播图样式 */
/* 如果手机系统webview的bug
    使用translate这行代码
*/
.swiper{
    border-radius: 10rpx;
    overflow: hidden;
    transform: translateY(0);
}
.swiper-item{
    display: flex;
}

.swiper-item .swiper-image{
    width: 100%;
}